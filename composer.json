{
  "name": "ivmak/chain-command",
  "version": "0.1.0",
  "type": "symfony-bundle",
  "description": "Chain Command Bundle, test task for ORO company",
  "license": "proprietary",
  "authors": [
    {
      "name": "Ivan Makarenkov",
      "email": "info@ivmak.com"
    }
  ],
  "require": {
    "php": "^8.0"
  },
  "require-dev": {
    "symfony/console": "^5.2",
    "symfony/dependency-injection": "^5.2",
    "symfony/framework-bundle": "^5.2",
    "symfony/http-kernel": "^5.2",
    "symfony/phpunit-bridge": "^5.2",
    "symfony/yaml": "^5.2"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Ivmak\\ChainCommandBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ivmak\\ChainCommandBundle\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test" : "./vendor/bin/simple-phpunit"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.0.*"
    }
  }
}
